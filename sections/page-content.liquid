<section class="page-default">
  <div class="container">
    {%- unless page.handle == 'cold-brew-campaign' -%}
      <h1>{{ page.title }}</h1>
    {%- endunless -%}
    <div class="rte">
      {{ page.content }}
    </div>
  </div>
</section>
{% schema %}
{
  "name": "Page Content",
  "settings": [],
  "presets": [
    {
      "name": "Default",
      "category": "Page"
    }
  ]
}
{% endschema %}
