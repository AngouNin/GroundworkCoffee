 .rc_subscription-only .subscription-selector-container .rc_widget__option__selector {
    background: #efbb52;
    border-radius: 10px;
    padding: 15px 30px !important;
    margin-bottom: 10px;
    width: 100%;
    position: relative;
}
.rc_widget__option__selector::before {
    transform: translateY(-50%);
    content: "";
    width: 18px;
    height: 18px;
    border-radius: 100%;
    position: absolute;
    top: 50%;
    background: #f2eee8;
    border: 1px solid #000;
    left: 30px;
}
.rc_widget__option__selector.active:after {
    content: "";
    width: 12px;
    height: 12px;
    border-radius: 100%;
    position: absolute;
    top: 50%;
    left: .3rem;
    background: #383938;
    border: 1px solid #000;
    transform: translateY(-50%);
    left: 33px;
}
 .rc_subscription-only .subscription-selector-container .d-flex {
    flex-wrap: wrap;
}
.rc-subscription-details {
    display: none !important;
}


